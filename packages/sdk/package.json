{
  "name": "@moneymq/sdk",
  "version": "0.7.0",
  "description": "MoneyMQ SDK for accepting stablecoin payments",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txtx/moneymq-js.git",
    "directory": "packages/sdk"
  },
  "keywords": [
    "moneymq",
    "payments",
    "stablecoin",
    "usdc",
    "solana",
    "x402"
  ],
  "author": "MoneyMQ",
  "license": "MIT",
  "dependencies": {
    "bs58": "^6.0.0"
  },
  "peerDependencies": {
    "x402-fetch": "^0.7.0"
  },
  "peerDependenciesMeta": {
    "x402-fetch": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20",
    "tsup": "^8.0.0",
    "typescript": "^5",
    "x402-fetch": "^0.7.0"
  }
}
